package main
import ("fmt")
func main(){
    var finalFee float64
    //Write your program logic here
    //Populate the variable: finalFee
    var course_fee float64=25000.0
    var mark float64=70
    var extra_fee float64=1500
    var scholarship float64=(mark/2)/100
    scholarship=course_fee*scholarship
    finalFee=course_fee-scholarship
    finalFee=finalFee+extra_fee


     //Do not modify the below print statement for verification to work
     fmt.Println(finalFee) 
}
